<h1>Search!</h1>

<nav class="users-search">
  <label>
    Username
    <input type="text" name="username">
  </label>

  <ul>
    <% @users.each do |user| %>
      <li>
        <%= link_to user.username, user_url(user) %>
        <%= render "follows/form", user: user %>
      </li>
    <% end %>
  </ul>

</nav>
